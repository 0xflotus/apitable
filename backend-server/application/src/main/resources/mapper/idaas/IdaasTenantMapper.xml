<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.enterprise.idaas.mapper.IdaasTenantMapper">
    <select id="selectByTenantName" resultType="com.vikadata.entity.IdaasTenantEntity">
        SELECT *
        FROM ${tablePrefix}idaas_tenant
        WHERE tenant_name = #{tenantName}
        AND is_deleted = 0
    </select>
</mapper>
