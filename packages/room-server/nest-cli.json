{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": [
      {
        "include": "**/*.yaml",
        "outDir": "./dist"
      },
      {
        "include": "./env/*",
        "outDir": "./dist"
      },
      {
        "include": "**/*.proto",
        "outDir": "./dist"
      },
      {
        "include": "**/*.lua",
        "outDir": "./dist"
      }
    ],
    "watchAssets": true
  }
}
