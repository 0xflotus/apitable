<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.modular.eco.mapper.EconomicOrderMetadataMapper">
    <select id="selectByOrderNos" resultType="com.vikadata.entity.EconomicOrderMetadataEntity">
        SELECT *
        FROM vika_economic_order_metadata
        WHERE order_no IN <foreach collection="orderNos" item="orderNo" open="(" separator="," close=")">#{orderNo}</foreach>
        AND is_deleted = 0
    </select>
</mapper>