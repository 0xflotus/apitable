<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.modular.workspace.mapper.NodeRoleMapper">

    <select id="selectResourceCodesByRoleCode" resultType="java.lang.String">
        SELECT resource_code
        FROM vika_node_role_resource_rel
        WHERE role_code = #{roleCode}
    </select>
</mapper>
