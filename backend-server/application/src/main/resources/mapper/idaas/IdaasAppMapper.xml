<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.enterprise.idaas.mapper.IdaasAppMapper">
    <select id="selectByClientId" resultType="com.vikadata.entity.IdaasAppEntity">
        SELECT *
        FROM ${tablePrefix}idaas_app
        WHERE client_id = #{clientId}
        AND is_deleted = 0
    </select>
</mapper>
