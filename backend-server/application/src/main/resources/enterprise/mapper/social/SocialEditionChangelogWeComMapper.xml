<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.vikadata.api.enterprise.social.mapper.SocialEditionChangelogWeComMapper">
    <select id="selectLastChangeLog" resultType="com.vikadata.api.enterprise.social.entity.SocialEditionChangelogWecomEntity">
        SELECT *
        FROM ${tablePrefix}social_edition_changelog_wecom
        WHERE suite_id = #{suiteId}
        AND paid_corp_id = #{paidCorpId}
        AND is_deleted = 0
        ORDER BY created_at DESC
        LIMIT 1
    </select>
</mapper>
