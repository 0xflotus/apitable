{
  "name": "@vikadata/websocket",
  "version": "0.13.5",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "bin": "dist/main.js",
  "pkg": {
    "scripts": "dist/**/*.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "grpc:build": "tsproto --path src/grpc/proto --output src/grpc/generated",
    "grpc:clean": "rimraf src/grpc/generated",
    "pm2:nest": "pm2-runtime start ecosystem.config.js"
  },
  "engines": {
    "node": "16.15.0"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.6.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/microservices": "^7.6.17",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/platform-socket.io": "^7.0.9",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/websockets": "^7.0.9",
    "@sentry/node": "^7.5.0",
    "@sentry/tracing": "^7.5.0",
    "grpc": "^1.24.10",
    "ioredis": "^4.16.3",
    "log4js": "^6.3.0",
    "nestjs-proto-gen-ts": "^1.0.16",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/ioredis": "^4.16.0",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-redis": "^1.0.26",
    "@types/supertest": "^2.0.8",
    "@types/uid2": "^0.0.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
